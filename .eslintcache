on = num.toFixed(2);\r\n        return Number(precision)\r\n    }\r\n   \r\n\r\n    return (\r\n        <div>\r\n            <h2>Order Summary</h2>\r\n            <h4>Items Ordered : {props.cart.length}</h4>\r\n            <p>Product Price : {formatNumber(total)}</p>\r\n            <p><small>Tax : {formatNumber(tax)}</small></p>\r\n            <p><small>Shipping Cost : {formatNumber(shipping)}</small></p>\r\n            <h3>Total Price : {formatNumber(grandTotal)}</h3>\r\n            <br/>\r\n           {\r\n               props.children\r\n           }\r\n            \r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Cart;","C:\\Development Website\\ema-john-simple\\src\\fakeData\\index.js",[],"C:\\Development Website\\ema-john-simple\\src\\fakeData\\camera.js",[],"C:\\Development Website\\ema-john-simple\\src\\fakeData\\android.js",[],"C:\\Development Website\\ema-john-simple\\src\\fakeData\\laptop.js",[],"C:\\Development Website\\ema-john-simple\\src\\components\\Login\\Login.js",["101"],"C:\\Development Website\\ema-john-simple\\src\\components\\Shipment\\Shipment.js",[],"C:\\Development Website\\ema-john-simple\\src\\components\\Login\\firebase.config.js",[],"C:\\Development Website\\ema-john-simple\\src\\components\\PrivateRoute\\PrivateRoute.js",["102"],{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":9,"column":3,"nodeType":"109","messageId":"110","endLine":9,"endColumn":7},{"ruleId":"107","severity":1,"message":"111","line":8,"column":12,"nodeType":"109","messageId":"110","endLine":8,"endColumn":24},{"ruleId":"107","severity":1,"message":"112","line":11,"column":20,"nodeType":"109","messageId":"110","endLine":11,"endColumn":31},{"ruleId":"107","severity":1,"message":"113","line":3,"column":27,"nodeType":"109","messageId":"110","endLine":3,"endColumn":39},{"ruleId":"107","severity":1,"message":"114","line":13,"column":21,"nodeType":"109","messageId":"110","endLine":13,"endColumn